{
  "name": "openclaw-space",
  "version": "1.0.0",
  "description": "在 GitHub Codespace 中快速部署和运行 OpenClaw。",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "openclaw gateway --verbose",
    "dev": "nodemon server.js",
    "serve": "nodemon server.js",
    "restart-gateway": "openclaw gateway restart",
    "run-ocw": "bash ocw.sh"
  },
  "keywords": [
    "openclaw",
    "codespace",
    "ai",
    "assistant"
  ],
  "author": "iam2r",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iam2r/openclaw-space.git"
  },
  "bugs": {
    "url": "https://github.com/iam2r/openclaw-space/issues"
  },
  "homepage": "https://github.com/iam2r/openclaw-space#readme",
  "dependencies": {
    "express": "^5.2.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  },
  "nodemonConfig": {
    "ext": "js,mjs,json",
    "ignore": [
      "node_modules/*",
      ".git/*",
      "public/*"
    ],
    "delay": 1000
  }
}
